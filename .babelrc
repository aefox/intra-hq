{
  "presets": [
    [
      "es2015",
      {
        "loose": true,
        "modules": false
      }
    ],
    "react"
  ],
  "env": {
    "production": {
      "presets": [
        "react-optimize"
      ]
    }
  },
  "plugins": [
    [
      "transform-react-jsx-source"
    ],
    [
      "transform-object-rest-spread",
      {
        "useBuiltIns": true
      }
    ],
    [
      "transform-decorators-legacy"
    ],
    [
      "transform-class-properties"
    ]
  ]
}
